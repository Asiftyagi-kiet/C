
//.........This program is Fahrenheit convert to celsius............
#include<stdio.h>
#include<conio.h>

void main()
{
    float c,f; //c denoted by Celsius   //f denoted by Fahrenheit

    printf("Enter in Fahrenheit temperature:\n");
    scanf("%f", &f);

    // Fahrenheit to celsius conversion formula */
     c = (f - 32) * 5 / 9;

    printf("%.2f Celsius",c);
    getch();

}
